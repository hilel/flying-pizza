    "@nestjs/platform-express": "^9.0.0",
    "@nrwl/angular": "15.9.2",
    "axios": "^1.0.0",
    "bootstrap": "^5.2.3",
    "express-session": "^1.17.3",
    "firebase": "^9.19.1",
